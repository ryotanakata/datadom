{
  "name": "@datadomjs/datadom",
  "version": "1.0.2",
  "description": "A lightweight utility for retrieving DOM elements using custom data attributes.",
  "type": "module",
  "exports": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryotanakata/datadom.git"
  },
  "keywords": [
    "DOM",
    "data-attributes",
    "query-selector",
    "element-selector",
    "client-side",
    "browser",
    "vanilla-js",
    "no-dependency",
    "lightweight",
    "performance",
    "custom-attributes",
    "web-development",
    "front-end",
    "DOM-manipulation",
    "utility-function"
  ],
  "author": "ryotanakata",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryotanakata/datadom/issues"
  },
  "homepage": "https://github.com/ryotanakata/datadom#readme",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jsdom": "^25.0.0",
    "prettier": "^3.3.3",
    "tsup": "^7.2.0",
    "typescript": "^5.6.2",
    "vitest": "^0.34.6"
  }
}
